{
  "name": "karting-frontend",
  "version": "1.0.0",
  "description": "Karting Dashboard Frontend - Vue 3 + TypeScript",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:check": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{vue,ts,js,json,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{vue,ts,js,json,css,scss}\"",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@sentry/vue": "^10.38.0",
    "@types/leaflet": "^1.9.21",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "leaflet": "^1.9.4",
    "pinia": "^3.0.3",
    "vue": "^3.5.22",
    "vue-chartjs": "^5.3.3",
    "vue-router": "^4.6.3",
    "vue-toastification": "^2.0.0-rc.5",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@pinia/testing": "^0.1.7",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^22.18.11",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^2.1.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^9.33.0",
    "happy-dom": "^15.0.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.8.1",
    "sass-embedded": "^1.96.0",
    "typescript": "~5.9.0",
    "vite": "^6.0.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vitest": "^2.1.0",
    "vue-tsc": "^3.1.1"
  }
}
